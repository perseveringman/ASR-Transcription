{
  "name": "aura",
  "version": "1.1.0",
  "description": "Voice transcription using Volcengine ASR service",
  "main": "dist/main.js",
  "scripts": {
    "start": "node esbuild.config.mjs",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "asr",
    "speech-to-text",
    "volcengine"
  ],
  "author": "perseveringman",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "0.19.12",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "eslint-plugin-sentence-case": "^0.0.6",
    "happy-dom": "^20.4.0",
    "obsidian": "latest",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  }
}
